    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.31",
    "typescript": "^4.8.4",
    "webpack": "^5.70.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
